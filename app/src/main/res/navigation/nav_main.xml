<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/login">

    <fragment
        android:id="@+id/login"
        android:name="com.example.freshmart.login"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_login_to_homeItemsFragment2"
            app:destination="@id/homeItemsFragment2"
            app:enterAnim="@anim/nav_default_pop_enter_anim"
            app:exitAnim="@anim/nav_default_pop_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/homeLogsFragment2"
        android:name="com.example.freshmart.HomeLogsFragment"
        android:label="fragment_home_logs"
        tools:layout="@layout/fragment_home_logs" >
        <action
            android:id="@+id/action_homeLogsFragment2_to_homeItemsFragment2"
            app:destination="@id/homeItemsFragment2"
            app:enterAnim="@anim/nav_default_pop_enter_anim"
            app:exitAnim="@anim/nav_default_pop_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/homeItemsFragment2"
        android:name="com.example.freshmart.HomeItemsFragment"
        android:label="fragment_home_items"
        tools:layout="@layout/fragment_home_items" >
        <action
            android:id="@+id/action_homeItemsFragment2_to_homeLogsFragment2"
            app:destination="@id/homeLogsFragment2"
            app:enterAnim="@anim/nav_default_pop_enter_anim"
            app:exitAnim="@anim/nav_default_pop_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
</navigation>